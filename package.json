{
    "name": "tgbot-printer",
    "version": "1.0.5",
    "description": "",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "format": "prettier --write \"(src|typings)/**/*.ts\"",
        "link": "npm run build && npm link",
        "lint": "eslint src/**/* --ext .ts",
        "lint-fix": "eslint src/**/* --ext .ts --fix",
        "postversion": "git push && git push --tags",
        "prepare": "npm run build",
        "prepublishOnly": "npm test && npm run lint",
        "preversion": "npm run format && npm run lint",
        "start": "node dist/index.js",
        "test": "",
        "test-coverage": "",
        "version": "npm run format && git add -A src"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:filiptypjeu/npm-tgobot-printer.git"
    },
    "homepage": "https://github.com/filiptypjeu/npm-tgobot-printer",
    "author": "Filip Stenbacka",
    "license": "ISC",
    "types": "dist/index.d.ts",
    "dependencies": {
        "@types/ipp": "^2.0.2",
        "@types/node-localstorage": "^1.3.0",
        "@types/node-telegram-bot-api": "^0.53.3",
        "ipp-easyprint": "^3.0.5",
        "node-fetch": "2.6.6",
        "node-localstorage": "^2.2.1",
        "node-telegram-bot-api": "^0.56.0",
        "tgbot-helpers": "^3.2.2",
        "tgbot-keyboard": "^1.0.0"
    },
    "devDependencies": {
        "@types/node-fetch": "2.5.12",
        "@typescript-eslint/eslint-plugin": "^4.29.3",
        "@typescript-eslint/parser": "^4.29.3",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "prettier": "^2.5.1",
        "typescript": "^4.5.4"
    },
    "files": [
        "dist"
    ],
    "keywords": [
        "ipp",
        "printer",
        "telegram",
        "tg",
        "bot",
        "tgbot"
    ]
}
